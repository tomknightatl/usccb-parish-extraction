# File: .gitignore

# Byte-compiled / optimized / DLL files
__pycache__/
*.py[cod]
*$py.class

# Jupyter Notebook checkpoints
.ipynb_checkpoints

# Environment variables and secrets
.env
*.env
secrets.json

# IDE files
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Data files that shouldn't be in git
data/outputs/
data/raw/
*.db
*.sqlite

# Logs
*.log
logs/

# Temporary files
tmp/
temp/
*.tmp

# ChromeDriver downloads
chromedriver*

# Test outputs
.pytest_cache/
htmlcov/
.coverage

# Distribution / packaging
.Python
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
wheels/
*.egg-info/
.installed.cfg
*.egg
